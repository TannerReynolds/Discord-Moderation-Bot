{
  "name": "discord-moderation-bot",
  "description": "A self hostable moderation bot for Discord",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "deploy": "cd src && node globalDeploy.js",
    "dev": "cd src && node --no-deprecation index.js --dev",
    "production": "cd src && pm2 start appMonitor.js --name botmonitor && pm2 start index.js --name modbot",
    "restart": "cd src && pm2 restart botmonitor && pm2 restart modbot",
    "validate": "cd src && node verifyInstall.js",
    "prisma": "npx prisma generate && npx prisma migrate dev --name modBot",
    "setup": "cd src && npm i && npm i -g prisma pm2 && node setupConfs.js && node verifyInstall.js && cd.. && npx prisma generate && npx prisma migrate dev --name modBot && cd src && node globalDeploy.js && node verifyInstall.js --complete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TannerReynolds/Discord-Moderation-Bot.git"
  },
  "keywords": [
    "discord",
    "moderator",
    "moderation",
    "bot",
    "discordjs",
    "self",
    "host",
    "hostable",
    "selfhostable"
  ],
  "author": "TannerReynolds",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TannerReynolds/Discord-Moderation-Bot/issues"
  },
  "homepage": "https://github.com/TannerReynolds/Discord-Moderation-Bot#readme",
  "version": "0.7.0",
  "dependencies": {
    "@lanred/discordjs-button-embed-pagination": "^5.0.2",
    "@prisma/client": "^5.9.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "confusables": "^1.1.1",
    "discord.js": "^14.9.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "longjohn": "^0.2.12",
    "luxon": "^3.3.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "body-parser": "^1.20.2",
    "ejs": "^3.1.9",
    "eslint": "^8.56.0",
    "eslint-config-fbjs": "^4.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "prisma": "^5.9.0"
  }
}
